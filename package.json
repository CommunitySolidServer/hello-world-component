{
  "name": "hello-world-module",
  "version": "4.0.0",
  "description": "A minimal example of a repository containing an injectable Community Solid Server module",
  "repository": "git@github.com:CommunitySolidServer/hello-world-module.git",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/hello-world-module",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/hello-world-module/^4.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/hello-world-module/^4.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/hello-world-module/^4.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/hello-world-module/^4.0.0/dist/": "dist/"
  },
  "scripts": {
    "start": "community-solid-server -c hello-world-file.json -f .data -m .",
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -i .componentsignore -r hello-world --typeScopedContexts",
    "build:ts": "tsc"
  },
  "files": [
    "dist",
    "config"
  ],
  "dependencies": {
    "@solid/community-server": "^4.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "componentsjs-generator": "^2.6.1",
    "typescript": "^4.7.4"
  },
  "license": "MIT"
}
